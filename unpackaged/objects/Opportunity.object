    <validationRules>
        <fullName>Backup</fullName>
        <active>false</active>
        <errorConditionFormula>OR( 
Ispickval(Priorvalue(StageName), &apos;Prospecting&apos;)&amp;&amp; !Ispickval((StageName), &apos;Qualification&apos;), 
Ispickval(Priorvalue(StageName), &apos;Qualification&apos;)&amp;&amp; !Ispickval((StageName), &apos;Needs Analysis&apos;),
Ispickval(Priorvalue(StageName), &apos;Needs Analysis&apos;)&amp;&amp; !Ispickval((StageName), &apos;Value Proposition&apos;),
Ispickval(Priorvalue(StageName), &apos;Value Proposition&apos;)&amp;&amp; !Ispickval((StageName), &apos;Id. Decision Makers&apos;),
Ispickval(Priorvalue(StageName), &apos;Id. Decision Makers&apos;)&amp;&amp; !Ispickval((StageName), &apos;Perception Analysis&apos;),
Ispickval(Priorvalue(StageName), &apos;Perception Analysis&apos;)&amp;&amp; !Ispickval((StageName), &apos;Proposal/Price quote&apos;),
Ispickval(Priorvalue(StageName), &apos;Proposal/Price quote&apos;)&amp;&amp; !Ispickval((StageName), &apos;Negotiation/Review&apos;),
Ispickval(Priorvalue(StageName), &apos;Negotiation/Review&apos;)&amp;&amp; !  or(ISPICKVAL(StageName, &apos;Closed won&apos;), 
ISPICKVAL(StageName,&apos;Closed Lost&apos;) ) 
)</errorConditionFormula>
        <errorMessage>Bakup</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Validation_Rules</fullName>
        <active>true</active>
        <errorConditionFormula>AND (NOT (ISNEW()),
OR( 
Ispickval(Priorvalue(StageName), &apos;Prospecting&apos;)&amp;&amp; !Ispickval(StageName, &apos;Qualification&apos;), 
Ispickval(Priorvalue(StageName), &apos;Qualification&apos;)&amp;&amp; !Ispickval((StageName), &apos;Needs Analysis&apos;),
Ispickval(Priorvalue(StageName), &apos;Needs Analysis&apos;)&amp;&amp; !Ispickval((StageName), &apos;Value Proposition&apos;),
Ispickval(Priorvalue(StageName), &apos;Value Proposition&apos;)&amp;&amp; !Ispickval((StageName), &apos;Id. Decision Makers&apos;),
Ispickval(Priorvalue(StageName), &apos;Id. Decision Makers&apos;)&amp;&amp; !Ispickval((StageName), &apos;Perception Analysis&apos;),
Ispickval(Priorvalue(StageName), &apos;Perception Analysis&apos;)&amp;&amp; !Ispickval((StageName), &apos;Proposal/Price quote&apos;),
Ispickval(Priorvalue(StageName), &apos;Proposal/Price quote&apos;)&amp;&amp; !Ispickval((StageName), &apos;Negotiation/Review&apos;),
Ispickval(Priorvalue(StageName), &apos;Negotiation/Review&apos;)&amp;&amp; !  or(ISPICKVAL(StageName, &apos;Closed won&apos;), 
ISPICKVAL(StageName,&apos;Closed Lost&apos;) ) 
))</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>navigation to the stages is not proper.Please check the document</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage_validation</fullName>
        <active>false</active>
        <errorConditionFormula>OR( 
Ispickval(Priorvalue(StageName), &apos;Prospecting&apos;)&amp;&amp; !Ispickval((StageName), &apos;Qualification&apos;), 
Ispickval(Priorvalue(StageName), &apos;Qualification&apos;)&amp;&amp; !Ispickval((StageName), &apos;Needs Analysis&apos;),
Ispickval(Priorvalue(StageName), &apos;Needs Analysis&apos;)&amp;&amp; !Ispickval((StageName), &apos;Value Proposition&apos;),
Ispickval(Priorvalue(StageName), &apos;Value Proposition&apos;)&amp;&amp; !Ispickval((StageName), &apos;Id. Decision Makers&apos;),
Ispickval(Priorvalue(StageName), &apos;Id. Decision Makers&apos;)&amp;&amp; !Ispickval((StageName), &apos;Perception Analysis&apos;),
Ispickval(Priorvalue(StageName), &apos;Perception Analysis&apos;)&amp;&amp; !Ispickval((StageName), &apos;Proposal/Price quote&apos;),
Ispickval(Priorvalue(StageName), &apos;Proposal/Price quote&apos;)&amp;&amp; !Ispickval((StageName), &apos;Negotiation/Review&apos;)
)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Process should be followed as per the tasks assigned by Sandeep Adhikari</errorMessage>
    </validationRules>
